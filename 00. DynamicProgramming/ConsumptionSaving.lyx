\begin_deeper
\begin_layout Itemize

\series bold
Market clearing
\series default
 requires
\begin_inset Formula 
\begin{align*}
\text{Capital: }K_{t} & =\mathcal{K}_{t}=\int a_{t}dD_{t+1}=\int a_{t}^{\ast}(e_{t},a_{t-1})dD_{t}\\
\text{Labour: }L_{t} & =\int e_{t}dD_{t}=1\\
\text{Goods: }Y_{t} & =\int c_{t}^{\ast}(e_{t},a_{t-1})dD_{t}+\delta K_{t-1}
\end{align*}

\end_inset


\end_layout

\begin_layout Itemize
The 
\series bold
labor market clears trivially
\series default
, while we can leave out the 
\series bold
goods market
\series default
 due to 
\series bold
Walras's Law
\end_layout

\end_deeper
