\begin_layout Itemize

\series bold
Approximate problem:
\series default

\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
vspace{-3mm}
\end_layout

\end_inset


\begin_inset Formula 
\begin{eqnarray*}
v(e_{t},a_{t-1},Z_{t},k_{t-1}) & = & \max_{c_{t}}\frac{c_{t}^{1-\sigma}}{1-\sigma}+\beta\mathbb{E}_{t}\left[v(e_{t+1},a_{t},Z_{t+1},k_{t})\right]\\
 & \text{s.t.}\\
a_{t}+c_{t} & = & (1+r_{t})a_{t-1}+w_{t}e_{t}\\
r_{t} & = & r(k_{t-1},Z_{t})\\
w_{t} & = & w(r_{t},Z_{t})\\
k_{t} & = & \text{PLM}(k_{t-1},Z_{t})\\
a_{t} & \geq & 0
\end{eqnarray*}

\end_inset

where 
\begin_inset Formula $\text{PLM}(k_{t-1},Z_{t})$
\end_inset

 is the 
\series bold
perceived law of motion
\begin_inset Newline newline
\end_inset

For example:
\series default
 
\begin_inset Formula $\text{PLM}(k_{t-1},Z_{t})=\beta_{Z_{t}}+\alpha_{Z_{t}}\log k_{t-1}$
\end_inset


\end_layout

