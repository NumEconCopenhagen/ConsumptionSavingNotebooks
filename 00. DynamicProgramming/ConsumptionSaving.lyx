\begin_inset Formula 
\begin{eqnarray*}
V_{0}(M_{0},P_{0}) & = & \max_{\{C_{t}\}_{t=0}^{T}}\sum_{t=0}^{T}\beta^{t}\frac{C_{t}^{1-\rho}}{1-\rho},\,\,\,\beta<1,\,\rho\geq1\\
 & \text{s.t.}\\
A_{t} & = & M_{t}-C_{t}\\
B_{t+1} & = & R\cdot A_{t},\,\,\,R>0\\
M_{t+1} & = & B_{t+1}+P_{t+1}\\
P_{t+1} & = & G\cdot P_{t},\,\,\,G>0\\
A_{T} & \geq & 0
\end{eqnarray*}

\end_inset


\end_layout

\end_deeper
