\begin_deeper
\begin_layout Itemize

\series bold
Substitution 
\series default
implies
\begin_inset Formula 
\begin{eqnarray*}
A_{T} & = & M_{T}-C_{T}=(RA_{T-1}+P_{T})-C_{T}\\
 & = & R(M_{T-1}-C_{T-1})+P_{T}-C_{T}\\
 & = & R^{2}A_{T-2}+RP_{T-1}-RC_{T-1}+P_{T}-C_{T}\\
 & = & R^{T+1}A_{-1}+\sum_{t=0}^{T}R^{T-t}(P_{t}-C_{t})
\end{eqnarray*}

\end_inset


\end_layout

\begin_layout Itemize
Use 
\series bold
terminal condition 
\series default

\begin_inset Formula $A_{T}=0$
\end_inset

 (why equality?)
\begin_inset Formula 
\begin{eqnarray*}
R^{-T}A_{T}=0 & \Leftrightarrow & B_{0}+H_{0}=\sum_{t=0}^{T}R^{-t}C_{t}
\end{eqnarray*}

\end_inset

where 
\begin_inset Formula $H_{0}\equiv\sum_{t=0}^{T}(G/R)^{t}P_{0}=\frac{1-(G/R)^{T+1}}{1-G/R}P_{0}$
\end_inset


\end_layout

\end_deeper
