\size footnotesize

\begin_inset Formula 
\begin{align*}
\mathcal{J}^{r,K} & =\alpha(\alpha-1)Z_{t}K_{t}^{\alpha-2}\begin{bmatrix}0 & 0 & \cdots & 0\\
1 & 0 & \cdots & 0\\
\vdots & 1 & 0 & \vdots\\
0 & 0 & \ddots & 0\\
0 & 0 & \cdots & 1
\end{bmatrix},\mathcal{J}^{r,Z}=\alpha K_{t}^{\alpha-1}\boldsymbol{I}\\
\mathcal{J}^{w,K} & =(1-\alpha)\alpha Z_{t}K_{t}^{\alpha-1}\begin{bmatrix}0 & 0 & \cdots & 0\\
1 & 0 & \cdots & 0\\
\vdots & 1 & 0 & \vdots\\
0 & 0 & \ddots & 0\\
0 & 0 & \cdots & 1
\end{bmatrix},\mathcal{J}^{w,Z}=(1-\alpha)K_{t}^{\alpha}\boldsymbol{I}
