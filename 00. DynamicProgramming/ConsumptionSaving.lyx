\begin_layout Itemize

\series bold
Extended problem:
\series default

\begin_inset Formula 
\begin{eqnarray*}
v(e_{t},a_{t-1},Z_{t},D_{t}) & = & \max_{c_{t}}\frac{c_{t}^{1-\sigma}}{1-\sigma}+\beta\mathbb{E}_{t}\left[v(e_{t+1},a_{t},Z_{t+1},D_{t+1})\right]\\
 & \text{s.t.}\\
a_{t}+c_{t} & = & (1+r_{t})a_{t-1}+w_{t}e_{t}\\
k_{t-1} & = & \int a_{t-1}dD_{t}\\
r_{t} & = & r(k_{t-1},Z_{t})\\
w_{t} & = & w(r_{t},Z_{t})\\
a_{t} & \geq & 0
\end{eqnarray*}

\end_inset


\end_layout

